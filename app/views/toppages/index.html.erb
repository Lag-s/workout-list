<% if logged_in? %>
  <div class="row">
    <div class="col-sm-8">
      <%= render 'workouts/workouts', workouts: @workouts %>
    </div>
  </div>
<% else %>
  <div class="center jumbotron">
    <div class="text-center">
      <h1>Welcome to the Workout-list</h1>
      <%= link_to '新規登録', signup_path, class: 'btn btn-lg btn-primary' %>
    </div>
  </div>
<% end %>